{
  "name": "@merrywhether/config",
  "version": "2.2.0",
  "description": "Common config files for @merrywhether projects",
  "bin": {
    "mw-config": "./index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/merrywhether/config.git"
  },
  "author": "merrywhether",
  "license": "MIT",
  "homepage": "https://github.com/merrywhether/config#readme",
  "scripts": {
    "fix": "yarn lint:es:fix && yarn lint:md:fix",
    "lint": "yarn lint:es && yarn lint:md",
    "lint:es": "eslint './'",
    "lint:es:fix": "yarn lint:es --fix",
    "lint:md": "yarn prettier '**/*.md' --check",
    "lint:md:fix": "yarn prettier '**/*.md' --write"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=v14.18.1"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "command-line-args": "^5.2.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-solid": "^0.1.2",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-typescript-sort-keys": "^2.0.0",
    "prettier": "^2.4.1",
    "typescript": "^4.4.4",
    "typescript-styled-plugin": "^0.18.1"
  }
}
